{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        // "linebreak-style": ["error", "windows"],
        "no-param-reassign": "off",
        "no-underscore-dangle": "off",
        "consistent-return": "off",
        "no-console": ["warn", { "allow": ["warn", "error"] }]
    }
}
